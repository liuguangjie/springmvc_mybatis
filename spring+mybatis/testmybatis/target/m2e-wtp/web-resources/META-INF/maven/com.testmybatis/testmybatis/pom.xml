<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.testmybatis</groupId>
	<artifactId>testmybatis</artifactId>
	<packaging>war</packaging>
	<version>0.0.1-SNAPSHOT</version>
	<name>testmybatis Maven Webapp</name>
	<url>http://maven.apache.org</url>
	<properties>
		<!-- 定义变量 以 project开头 引用变量 ${project.xxxxx} -->
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<project.finalName>testmybatis</project.finalName>
		<project.lib>src/main/webapp/WEB-INF/lib</project.lib>
		<project.jdkversion>1.8</project.jdkversion>
		<project.maven.compiler>3.1</project.maven.compiler>
	</properties>
	<build>
		<finalName>${project.finalName}</finalName>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>${project.maven.compiler}</version>
				<configuration>
					<source>${project.jdkversion}</source>
					<target>${project.jdkversion}</target>
					<encoding>${project.build.sourceEncoding}</encoding>  <!-- “编码 GBK 的不可映射字符”问题的解决 -->
					<compilerArguments>
						<extdirs>${project.lib}</extdirs>
					</compilerArguments>
				</configuration>
			</plugin>
		</plugins>
	</build>


